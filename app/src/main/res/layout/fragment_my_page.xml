<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".MainTopMenuItems.HomeFragment">

    <ScrollView
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <androidx.constraintlayout.utils.widget.ImageFilterView
                android:id="@+id/profileImageView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="16dp"
                android:layout_marginTop="20dp"
                android:src="@drawable/profile_none"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/loginButton"
                android:layout_width="66dp"
                android:layout_height="25dp"
                android:background="@drawable/rectangle_dawn_color"
                android:text="로그인"
                android:textSize="11sp"
                android:textColor="@color/white"
                app:layout_constraintStart_toEndOf="@id/profileImageView"
                app:layout_constraintTop_toTopOf="@id/profileImageView"
                android:layout_marginTop="16dp"
                android:layout_marginStart="12dp"/>
            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/registerButton"
                android:layout_width="66dp"
                android:layout_height="25dp"
                android:text="회원가입"
                android:textSize="11sp"
                android:background="@drawable/register_button"
                app:layout_constraintStart_toEndOf="@id/loginButton"
                app:layout_constraintTop_toTopOf="@id/loginButton"
                app:layout_constraintBottom_toBottomOf="@id/loginButton"
                android:layout_marginStart="5dp" />

            <androidx.appcompat.widget.LinearLayoutCompat
                android:id="@+id/pointButton"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                android:layout_marginTop="15dp"
                android:layout_marginEnd="31dp"
                android:gravity="center">
                <androidx.constraintlayout.utils.widget.ImageFilterView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/point"
                    android:layout_marginEnd="7dp"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="포인트"
                    android:textColor="@color/text_color_gray"
                    android:textSize="12sp"
                    android:textStyle="bold"
                    android:layout_marginEnd="14dp"/>
                <androidx.constraintlayout.utils.widget.ImageFilterView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/right_arrow"/>
            </androidx.appcompat.widget.LinearLayoutCompat>
            <androidx.appcompat.widget.LinearLayoutCompat
                android:id="@+id/couponButton"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@id/pointButton"
                android:layout_marginTop="28dp"
                android:layout_marginEnd="31dp"
                android:gravity="center">
                <androidx.constraintlayout.utils.widget.ImageFilterView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/coupon"
                    android:layout_marginEnd="9dp"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="쿠폰"
                    android:textColor="@color/text_color_gray"
                    android:textSize="12sp"
                    android:textStyle="bold"
                    android:layout_marginEnd="19dp"/>
                <androidx.constraintlayout.utils.widget.ImageFilterView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/right_arrow"/>
            </androidx.appcompat.widget.LinearLayoutCompat>

            <FrameLayout
                android:id="@+id/myPageMenuFrameLayout"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@id/couponButton"
                android:layout_marginTop="28dp">
                <androidx.constraintlayout.utils.widget.ImageFilterView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:src="@drawable/grid_line"
                    android:scaleType="fitXY"/>

                <GridLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:columnCount="3"
                    android:rowCount="2">

                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:id="@+id/QnAButton"
                        android:layout_width="0dp"
                        android:layout_height="0dp"
                        android:layout_columnWeight="1"
                        android:layout_rowWeight="1">

                        <androidx.constraintlayout.utils.widget.ImageFilterView
                            android:id="@+id/QnAIcon"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:src="@drawable/qna"
                            app:layout_constraintTop_toTopOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintEnd_toEndOf="parent"
                            android:layout_marginTop="15dp"/>

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Q&amp;A"
                            android:textSize="11sp"
                            android:textStyle="bold"
                            android:textColor="@color/text_color_gray"
                            app:layout_constraintTop_toBottomOf="@id/QnAIcon"
                            app:layout_constraintStart_toStartOf="@id/QnAIcon"
                            app:layout_constraintEnd_toEndOf="@id/QnAIcon"
                            android:layout_marginTop="2dp"/>

                        <androidx.constraintlayout.utils.widget.ImageFilterView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:src="@drawable/right_arrow"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintTop_toTopOf="parent"
                            app:layout_constraintBottom_toBottomOf="parent"
                            android:layout_marginEnd="13dp"/>

                    </androidx.constraintlayout.widget.ConstraintLayout>
                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:id="@+id/cartButton"
                        android:layout_width="0dp"
                        android:layout_height="0dp"
                        android:layout_columnWeight="1"
                        android:layout_rowWeight="1">
                        <androidx.constraintlayout.utils.widget.ImageFilterView
                            android:id="@+id/cartIcon"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:src="@drawable/cart"
                            app:layout_constraintTop_toTopOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintEnd_toEndOf="parent"
                            android:layout_marginTop="15dp"/>

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="장바구니"
                            android:textSize="11sp"
                            android:textStyle="bold"
                            android:textColor="@color/text_color_gray"
                            app:layout_constraintTop_toBottomOf="@id/cartIcon"
                            app:layout_constraintStart_toStartOf="@id/cartIcon"
                            app:layout_constraintEnd_toEndOf="@id/cartIcon"
                            android:layout_marginTop="2dp"/>

                        <androidx.constraintlayout.utils.widget.ImageFilterView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:src="@drawable/right_arrow"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintTop_toTopOf="parent"
                            app:layout_constraintBottom_toBottomOf="parent"
                            android:layout_marginEnd="13dp"/>

                    </androidx.constraintlayout.widget.ConstraintLayout>
                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:id="@+id/reviewButton"
                        android:layout_width="0dp"
                        android:layout_height="0dp"
                        android:layout_columnWeight="1"
                        android:layout_rowWeight="1">
                        <androidx.constraintlayout.utils.widget.ImageFilterView
                            android:id="@+id/reviewIcon"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:src="@drawable/message_bubble"
                            app:layout_constraintTop_toTopOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintEnd_toEndOf="parent"
                            android:layout_marginTop="15dp"/>

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="리뷰 작성"
                            android:textSize="11sp"
                            android:textStyle="bold"
                            android:textColor="@color/text_color_gray"
                            app:layout_constraintTop_toBottomOf="@id/reviewIcon"
                            app:layout_constraintStart_toStartOf="@id/reviewIcon"
                            app:layout_constraintEnd_toEndOf="@id/reviewIcon"
                            android:layout_marginTop="2dp"/>

                        <androidx.constraintlayout.utils.widget.ImageFilterView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:src="@drawable/right_arrow"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintTop_toTopOf="parent"
                            app:layout_constraintBottom_toBottomOf="parent"
                            android:layout_marginEnd="13dp"/>

                    </androidx.constraintlayout.widget.ConstraintLayout>
                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:id="@+id/setAvatarButton"
                        android:layout_width="0dp"
                        android:layout_height="0dp"
                        android:layout_columnWeight="1"
                        android:layout_rowWeight="1">
                        <androidx.constraintlayout.utils.widget.ImageFilterView
                            android:id="@+id/avatarIcon"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:src="@drawable/set_avatar"
                            app:layout_constraintTop_toTopOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintEnd_toEndOf="parent"
                            android:layout_marginTop="15dp"/>

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="아바타 설정"
                            android:textSize="11sp"
                            android:textStyle="bold"
                            android:textColor="@color/text_color_gray"
                            app:layout_constraintTop_toBottomOf="@id/avatarIcon"
                            app:layout_constraintStart_toStartOf="@id/avatarIcon"
                            app:layout_constraintEnd_toEndOf="@id/avatarIcon"
                            android:layout_marginTop="2dp"/>

                        <androidx.constraintlayout.utils.widget.ImageFilterView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:src="@drawable/right_arrow"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintTop_toTopOf="parent"
                            app:layout_constraintBottom_toBottomOf="parent"
                            android:layout_marginEnd="13dp"/>

                    </androidx.constraintlayout.widget.ConstraintLayout>
                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:id="@+id/closetButton"
                        android:layout_width="0dp"
                        android:layout_height="0dp"
                        android:layout_columnWeight="1"
                        android:layout_rowWeight="1">
                        <androidx.constraintlayout.utils.widget.ImageFilterView
                            android:id="@+id/boxIcon"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:src="@drawable/box"
                            app:layout_constraintTop_toTopOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintEnd_toEndOf="parent"
                            android:layout_marginTop="15dp"/>

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="옷장"
                            android:textSize="11sp"
                            android:textStyle="bold"
                            android:textColor="@color/text_color_gray"
                            app:layout_constraintTop_toBottomOf="@id/boxIcon"
                            app:layout_constraintStart_toStartOf="@id/boxIcon"
                            app:layout_constraintEnd_toEndOf="@id/boxIcon"
                            android:layout_marginTop="2dp"/>

                        <androidx.constraintlayout.utils.widget.ImageFilterView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:src="@drawable/right_arrow"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintTop_toTopOf="parent"
                            app:layout_constraintBottom_toBottomOf="parent"
                            android:layout_marginEnd="13dp"/>

                    </androidx.constraintlayout.widget.ConstraintLayout>
                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:id="@+id/codyInquiryButton"
                        android:layout_width="0dp"
                        android:layout_height="0dp"
                        android:layout_columnWeight="1"
                        android:layout_rowWeight="1">
                        <androidx.constraintlayout.utils.widget.ImageFilterView
                            android:id="@+id/codyIcon"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:src="@drawable/cody_inquiry"
                            app:layout_constraintTop_toTopOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintEnd_toEndOf="parent"
                            android:layout_marginTop="15dp"/>

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="코디 문의"
                            android:textSize="11sp"
                            android:textStyle="bold"
                            android:textColor="@color/text_color_gray"
                            app:layout_constraintTop_toBottomOf="@id/codyIcon"
                            app:layout_constraintStart_toStartOf="@id/codyIcon"
                            app:layout_constraintEnd_toEndOf="@id/codyIcon"
                            android:layout_marginTop="2dp"/>

                        <androidx.constraintlayout.utils.widget.ImageFilterView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:src="@drawable/right_arrow"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintTop_toTopOf="parent"
                            app:layout_constraintBottom_toBottomOf="parent"
                            android:layout_marginEnd="13dp"/>

                    </androidx.constraintlayout.widget.ConstraintLayout>

                </GridLayout>
            </FrameLayout>

            <TextView
                android:id="@+id/orderItemsTitleTextView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintTop_toBottomOf="@id/myPageMenuFrameLayout"
                app:layout_constraintStart_toStartOf="parent"
                android:text="주문내역"
                android:textStyle="bold"
                android:textColor="@color/titleColor_black"
                android:textSize="16sp"
                android:layout_marginTop="20dp"
                android:layout_marginStart="22dp"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="(최근 3개월)"
                android:textSize="8sp"
                android:textColor="#929292"
                app:layout_constraintStart_toEndOf="@id/orderItemsTitleTextView"
                app:layout_constraintBottom_toBottomOf="@id/orderItemsTitleTextView"
                android:layout_marginStart="3dp"/>

            <androidx.constraintlayout.utils.widget.ImageFilterView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/right_arrow"
                app:layout_constraintTop_toTopOf="@id/orderItemsTitleTextView"
                app:layout_constraintBottom_toBottomOf="@id/orderItemsTitleTextView"
                app:layout_constraintEnd_toEndOf="@+id/orderItemLayout"
                android:layout_marginEnd="3dp"/>


            <androidx.appcompat.widget.LinearLayoutCompat
                android:id="@+id/orderItemLayout"
                android:layout_width="0dp"
                android:layout_height="70dp"
                app:layout_constraintTop_toBottomOf="@id/orderItemsTitleTextView"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                android:layout_marginStart="23dp"
                android:layout_marginEnd="23dp"
                android:layout_marginTop="10dp"
                android:background="@drawable/rectangle_dawn_color"
                android:backgroundTint="#1133BFBE"
                android:gravity="center">

                <androidx.appcompat.widget.LinearLayoutCompat
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:layout_marginEnd="35dp">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="입금/결제"
                        android:textSize="12sp"
                        android:textColor="@color/titleColor_black"/>

                    <TextView
                        android:id="@+id/paymentCountTextView"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="-"
                        android:textSize="15sp"
                        android:textColor="@color/titleColor_black"
                        android:textStyle="bold"
                        android:layout_gravity="center_horizontal"/>

                </androidx.appcompat.widget.LinearLayoutCompat>
                <androidx.appcompat.widget.LinearLayoutCompat
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:layout_marginEnd="35dp">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="배송중"
                        android:textSize="12sp"
                        android:textColor="@color/titleColor_black"/>

                    <TextView
                        android:id="@+id/deliverCountTextView"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="-"
                        android:textSize="15sp"
                        android:textColor="@color/titleColor_black"
                        android:textStyle="bold"
                        android:layout_gravity="center_horizontal"/>

                </androidx.appcompat.widget.LinearLayoutCompat>
                <androidx.appcompat.widget.LinearLayoutCompat
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:layout_marginEnd="35dp">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="배송완료"
                        android:textSize="12sp"
                        android:textColor="@color/titleColor_black"/>

                    <TextView
                        android:id="@+id/DeliveryCompletedCountTextView"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="-"
                        android:textSize="15sp"
                        android:textColor="@color/titleColor_black"
                        android:textStyle="bold"
                        android:layout_gravity="center_horizontal"/>

                </androidx.appcompat.widget.LinearLayoutCompat>
                <androidx.appcompat.widget.LinearLayoutCompat
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="취소/반품"
                        android:textSize="12sp"
                        android:textColor="@color/titleColor_black"/>

                    <TextView
                        android:id="@+id/CancelCountTextView"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="-"
                        android:textSize="15sp"
                        android:textColor="@color/titleColor_black"
                        android:textStyle="bold"
                        android:layout_gravity="center_horizontal"/>

                </androidx.appcompat.widget.LinearLayoutCompat>
            </androidx.appcompat.widget.LinearLayoutCompat>

            <TextView
                android:id="@+id/recentlyLookedTitleTextView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintTop_toBottomOf="@id/orderItemLayout"
                app:layout_constraintStart_toStartOf="parent"
                android:text="최근 본 상품"
                android:textStyle="bold"
                android:textColor="@color/titleColor_black"
                android:textSize="16sp"
                android:layout_marginTop="20dp"
                android:layout_marginStart="22dp"/>


            <androidx.constraintlayout.utils.widget.ImageFilterView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/right_arrow"
                app:layout_constraintTop_toTopOf="@id/recentlyLookedTitleTextView"
                app:layout_constraintBottom_toBottomOf="@id/recentlyLookedTitleTextView"
                app:layout_constraintEnd_toEndOf="@+id/orderItemLayout"
                android:layout_marginEnd="3dp"/>

            <androidx.recyclerview.widget.RecyclerView
                android:id="@+id/recentlyLookedRecyclerView"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@id/recentlyLookedTitleTextView"
                app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
                android:layout_marginTop="10dp"
                android:layout_marginStart="22dp"
                android:layout_marginEnd="22dp"/>

            <View
                android:id="@+id/line1"
                android:layout_width="0dp"
                android:layout_height="1dp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@id/recentlyLookedRecyclerView"
                android:layout_marginTop="20dp"
                android:background="#DFDFDF"
                android:layout_marginStart="22dp"
                android:layout_marginEnd="22dp"/>

            <androidx.appcompat.widget.LinearLayoutCompat
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@id/line1"
                android:layout_marginTop="15dp"
                android:layout_marginStart="22dp"
                android:layout_marginEnd="22dp">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="고객센터"
                    android:textSize="12sp"
                    android:textColor="#6D6D6D"/>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="문의 내역"
                    android:textSize="15sp"
                    android:textColor="@color/titleColor_black"
                    android:textStyle="bold"
                    android:layout_marginTop="15dp"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="FAQ"
                    android:textSize="15sp"
                    android:textColor="@color/titleColor_black"
                    android:textStyle="bold"
                    android:layout_marginTop="15dp"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="공지사항"
                    android:textSize="15sp"
                    android:textColor="@color/titleColor_black"
                    android:textStyle="bold"
                    android:layout_marginTop="15dp"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="앱 문의 · 건의"
                    android:textSize="15sp"
                   android:textColor="@color/titleColor_black"
                    android:textStyle="bold"
                    android:layout_marginTop="15dp"/>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_marginTop="15dp"
                    android:background="#DFDFDF"/>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="서비스"
                    android:textSize="12sp"
                    android:textColor="#6D6D6D"
                    android:layout_marginTop="15dp"/>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="실험실"
                    android:textSize="15sp"
                    android:textColor="@color/titleColor_black"
                    android:textStyle="bold"
                    android:layout_marginTop="15dp"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="고객 참여 설문"
                    android:textSize="15sp"
                    android:textColor="@color/titleColor_black"
                    android:textStyle="bold"
                    android:layout_marginTop="15dp"/>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_marginTop="15dp"
                    android:background="#DFDFDF"/>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="기타"
                    android:textSize="12sp"
                    android:textColor="#6D6D6D"
                    android:layout_marginTop="15dp"/>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="개인정보 수집 및 이용"
                    android:textSize="15sp"
